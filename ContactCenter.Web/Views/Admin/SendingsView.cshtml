@using ContactCenter.Core.Models
@using Microsoft.AspNetCore.Identity
@model LoginViewModel
@inject SignInManager<ApplicationUser> SignInManager
@{
	ViewData["Title"] = "Contact Center | Admin";
	Layout = "_Layout";
}

<script src="~/assets-admin/scripts/Sendings.js?ver=1.13"></script>

<p class="app-sidebar__heading">Envios</p>

<div class="col-md-12">
	<table id="sendings-grid" class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>Tipo</th>
				<th>Status</th>
				<th>Título</th>
				<th>Data</th>
				<th>Contatos</th>
				<th>Enviados</th>
				<th style="text-align: right;">
					Ação
					<a title="Novo Envio"
					   data-toggle="modal"
					   data-target="#modal-sending"
					   class="add-row btn btn-primary"
					   aria-describedby="Novo envio"
					   onclick="clearSendingModal()">
						<i class="fa fa-plus"></i>
					</a>
				</th>
			</tr>
		</thead>
		<tbody>
		</tbody>
	</table>
</div>